function main() {
    let a = 500 as saturated u8
    let b = 257 as truncated u8
    let c = 127 as! i8
    let d = -127 as! i8
    let e = -200 as saturated i8
    let f = -127 as truncated i8
    let g = 128 as truncated i8

    // as Should not eat the token after the type
    let h = (1 as! i8)

    println("{}", a)
    println("{}", b)
    println("{}", c)
    println("{}", d)
    println("{}", e)
    println("{}", f)
    println("{}", g)
    println("{}", h)
}
