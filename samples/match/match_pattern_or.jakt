/// Expect:
/// - output: "PASS\nPASS\n"

enum Foo {
    Bar
    Baz
    Bat
}

fn main() {
    println("{}", match "foo" {
        "foo" | "bar" | "baz" => {
            yield "PASS"
        }
        else => "FAIL"
    })

    let p = Foo::Bar
    println("{}", match p {
        Bar => "PASS"
        Baz | else => "FAIL"
    })
}
