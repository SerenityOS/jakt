/// Expect:
/// - output: "I found an A 1\nJust 2\n"

enum Foo {
    A(x: i64)
    B(x: i64)
    C(x: i64)
}

fn main() {
    let a = Foo::A(x: 1)

    match a {
        A(x) => println("I found an A {}", x)
        else(x) => println("Just {}", x)
    }

    let b = Foo::B(x: 2)

    match b {
        A(x) => println("I found an A {}", x)
        else(x) => println("Just {}", x)
    }
}
