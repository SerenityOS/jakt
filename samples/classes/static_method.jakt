class Person {
    public name: String
    public age: i64

    public function generate(name: String, age: i64) throws -> Person {
        return Person(name: name, age: age + 1000);
    }
}

function main() {
    let p = Person::generate(name: "Jane", age: 100)

    println("{}", p.name)
    println("{}", p.age)
}
