/// Expect:
/// - output: "PASS\n0\n2\n4\n6\n8\n"

fn test1(anon x: i64) throws -> String {
    guard x == 1 else {
        return "FAIL"
    }

    return "PASS"
}

fn test2() throws {
    mut i = 0
    loop {
        guard i % 2 == 0 else {
            i++
            continue
        }
        guard i < 10 else {
            break
        }

        println("{}", i++)
    }
}

fn main() {
    println("{}", test1(1))
    test2()
}
