class Foo {
    x: i64

    public function set(mutable this, value: i64) {
        this.x = value
    }

    public function get(this) {
        return this.x
    }
}

function main() {
    let mutable foo = Foo(x: 1)
    let mutable opt_foo: Foo? = foo

    opt_foo!.set(value: 2)

    println("{}", opt_foo!.get())
}
