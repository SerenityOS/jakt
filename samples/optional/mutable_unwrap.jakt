class Foo {
    x: i64

    function set(mutable this, value: i64) {
        this.x = value
    }

    function get(this) {
        return this.x
    }
}

function main() {
    try {
        let mutable foo = Foo(x: 1)
        let mutable opt_foo: Foo? = foo

        opt_foo!.set(value: 2)

        println("{}", opt_foo!.get())
    } catch err {}
}
