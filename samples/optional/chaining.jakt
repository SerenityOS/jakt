class Foo {
    public bar: Bar
}
class Bar {
    public baz: Baz
}
class Baz {
    public x: String
}

function main() {
    let foo: Foo? = Foo(bar: Bar(baz: Baz(x: "well hello friends :^)")))
    let null_foo: Foo? = None

    println("{}", foo?.bar?.baz?.x ?? "no value")
    println("{}", null_foo?.bar?.baz?.x ?? "no value")
}