/// Expect:
/// - output: "200\n2000\n200000\n125\n"

struct Foo {
    x: u16
}

fn main() {
    mut a: u8 = 100
    a += 100

    mut b: u16 = 1000
    b += 1000

    mut c: u32 = 100000
    c += 100000

    mut f = Foo(x: 123)
    f.x += 2

    println("{}", a)
    println("{}", b)
    println("{}", c)
    println("{}", f.x)
}
