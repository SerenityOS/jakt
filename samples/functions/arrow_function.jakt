/// Expect:
/// - output: "50\nHello, friends!\n"

fn area(width: i64, height: i64) => width * height
fn greeting(name: String) throws -> String => format("Hello, {}!", name)
fn greet(name: String) throws {
    println("{}", greeting(name))
} 

fn main() {
    let a = area(width: 5, height: 10)
    println("{}", a)

    greet(name: "friends")
}
