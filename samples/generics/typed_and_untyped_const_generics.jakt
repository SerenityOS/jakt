/// Expect:
/// - output: "typed: 10\nuntyped: 20\n"

fn typed_version<comptime N: i32>() -> i32 {
    return N
}

fn untyped_version<comptime N>() -> i32 {
    return N
}

fn main() {
    println("typed: {}", typed_version<comptime 10i32>())
    println("untyped: {}", untyped_version<comptime 20i32>())
}
