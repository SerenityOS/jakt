/// Expect:
/// - output: "hello 5\n"

comptime id<T>(anon x: T) -> T => x

fn format_with_count<T, comptime N: i32>(anon value: T) throws -> String {
    let count = id(N) as! i32
    return format("{} {}", value, count)
}

fn main() {
    println("{}", format_with_count<String, comptime 5i32>("hello"))
}
