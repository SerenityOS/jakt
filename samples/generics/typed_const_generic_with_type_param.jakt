/// Expect:
/// - output: "hello 5\n"

comptime id<T>(anon x: T) -> T => x

fn format_with_count<comptime N: i32, comptime value: String>() throws -> String {
    let count = N
    return format("{} {}", value, count)
}

fn main() {
    println("{}", format_with_count<comptime 5i32, comptime "hello">())
}
