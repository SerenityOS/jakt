struct Rectangle {
    width: i64,
    height: i64,

    function grow(mutable this) {
        this.width *= 2;
        this.height *= 2;
    }

    function area(this) => this.width * this.height
}

function main() {
    let mutable rect = Rectangle(width: 4, height: 8)

    rect.grow()

    println("{}", rect.area())
}