/// Expect:
/// - output: "5\n5\n5\n"

struct Foo {
    x: i64
}

function set_x(foo: mutable Foo, x: i64) {
    foo.x = x
}

function main() {
    let foo = Foo(x: 5)
    println("{}", foo.x)

    set_x(foo: foo, x: 10)
    println("{}", foo.x)

    let mutable bar = foo
    bar.x = 15
    println("{}", foo.x)
}
