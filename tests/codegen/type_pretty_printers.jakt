/// Expect:
/// - output: "Value::Bar(values: [\"hello\"])\nRect(location: Point(x: 0, y: 0), size: Size(width: 15, height: 30))\nFoo::Baz(100)\n"

class Size {
    pub width: i64
    pub height: i64
}

class Point {
    pub x: i64
    pub y: i64
}

class Rect {
    pub location: Point
    pub size: Size
}

enum Value {
    Foo
    Bar(values: [String])
}

enum Foo {
    Bar
    Baz(i64)
}

fn main() {
    let e = Value::Bar(values: ["hello"])
    println("{}", e)

    let r = Rect(location: Point(x: 0, y: 0), size: Size(width: 15, height: 30))
    println("{}", r)

    let foo = Foo::Baz(100)
    println("{}", foo)
}
