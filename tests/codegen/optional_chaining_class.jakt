/// Expect:
/// - output: "PASS\n123\nPASS\nNone\n"

class Test {
    pub x: i32

    pub fn y(this) throws => this
    pub fn throwing_y(this) throws -> String {
        throw Error::from_errno(69)
    }
}

fn main() {
    mut test: Test? = Test(x: 123)
    try {
        println("{}", test?.y()?.throwing_y() ?? "FAIL")
    } catch err {
        println("PASS")
    }

    println("{}", test?.x)

    test = None
    try {
        println("{}", test?.y()?.throwing_y() ?? "PASS")
    } catch err {
        println("FAIL")
    }

    println("{}", test?.x)
}
