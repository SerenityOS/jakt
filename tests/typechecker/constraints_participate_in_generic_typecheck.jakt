/// Expect:
/// - output: "OK\n"

trait Trait<T> {
    fn bar(this) -> T
}

struct Thing implements(Trait<u32>) {
    fn bar(this) -> u32 => 1337u32
}

class Class<T requires(Trait<u32>)> {
    pub fn foo(this, anon v: T) {
        let bar = v.bar()

        if bar == 1337 {
            println("OK")
        }
    }
}

fn main() {
    let c: Class<Thing> = Class()
    c.foo(Thing())
}
