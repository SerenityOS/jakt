/// Expect:
/// - error: "Missing override keyword on function that is virtual\n"

class Parent {
    pub virtual fn greet(this) {
        println("greeting from the parent")
    }
}

class Child: Parent {
    pub fn greet(this) {
        println("greeting from the child")
    }
}

fn use_parent(anon parent: Parent) {
    parent.greet()
}

fn main() {
    let child = Child()

    use_parent(child)
}
