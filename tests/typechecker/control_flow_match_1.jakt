/// Expect:
/// - output: "PASS\n"

fn test(first: i64, second: i64) -> String {
    match first {
        1 => {
            // Partially never return
            if second == 2 {
                abort()
            }
        }
        else => {
            // Always return
            return "NOT PASSED"
        }
    }

    return "PASS" // This should be reachable (in case first == 1 and second != 2)
}

fn main() {
    println("{}", test(first: 1, second: 0))
}
