/// Expect:
/// - error: "Binary arithmetic operation between incompatible types (`String` and `i64`)\n"

struct Foo<T> {
    a: T

    function test<U>(this, anon x: U) -> T {
        return this.a + x
    }
}

function main() {
    let f = Foo(a: "foo")
    f.test(123) // Can't add String and i64
}

